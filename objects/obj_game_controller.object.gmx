gamepad_set_axis_deadzone(player_light,.01);
gamepad_set_axis_deadzone(player_dark,.01);

//debug
debug = false;

//view controller
instance_create(0,0,obj_view_controller);

//background depth(behind everything) 
depth = 10;

//create bases and players
event_user(player_dark);
event_user(player_light);
event_user(2);



