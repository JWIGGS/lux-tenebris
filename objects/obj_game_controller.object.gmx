//base spawn
spawnBasePosition = 256;
spawnPlayerPosition = 256;

//player object
global.playerObject[player_dark] = -1;
global.playerObject[player_light] = -1;

//grid
global.gridSize = 16;
global.gridWidth = room_width/global.gridSize;
global.gridHeight = room_height/global.gridSize;
global.grid[0,0] = 0;
for(i = 0; i&lt;global.gridWidth; i++){
    for(j = 0; j&lt;global.gridHeight; j++){
        if(i&lt;global.gridWidth/2){
            global.grid[i,j] = player_light;
        }
        else{
            global.grid[i,j] = player_dark;
        }
    }
}

