            <string>///movement

//change speed variables

//left hat

/*
//Movement A
spdX = clamp(spdX+(gamepad_axis_value(player,gp_axislh)*spdDelta),-spdMax,spdMax);
spdY = clamp(spdY+(gamepad_axis_value(player,gp_axislv)*spdDelta),-spdMax,spdMax);
*/

/*
//Movement B
spdX = lengthdir_x(spdMax,targetAngle);
spdY = lengthdir_y(spdMax,targetAngle);
*/


//Movement C
spdX = lengthdir_x((spdMax/2)+((spdMax/2)*gamepad_button_check(player,gp_shoulderl)),targetAngle);
spdY = lengthdir_y((spdMax/2)+((spdMax/2)*gamepad_button_check(player,gp_shoulderl)),targetAngle);

/*
//Movement D
spdX = lengthdir_x(spdMax,targetAngle);
spdY = lengthdir_y(spdMax,targetAngle);
*/


//x deceleration if no x axis gamepad movement
if(abs(gamepad_axis_value(player,gp_axislh)) &lt;.2){
    if( abs(spdX)&gt;spdDelta){
        spdX -= spdDelta*sign(spdX);
    }
    else{
        spdX = 0;
    }
}

//y deceleration if no y axis gamepad movement
if(abs(gamepad_axis_value(player,gp_axislv)) &lt;.2){
    if( abs(spdY)&gt;spdDelta){
        spdY -= spdDelta*sign(spdY);
    }
    else{
        spdY = 0;
    }
}


//change x and y clamped to stay inside room
x = clamp(x+spdX,0,room_width);
y = clamp(y+spdY,0,room_height);

</string>
